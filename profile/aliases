# Processes
alias tu='top -o cpu' # cpu
alias tm='top -o vsize' # memory
alias e='exit'
alias k9="killall -9"

# RVM
alias rvmu="rvm use"

# some more ls aliases
alias ll='ls -alF'
alias la='ls -A'
